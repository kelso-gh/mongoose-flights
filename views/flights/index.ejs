<%- include('../partials/header') %>

<table id="list">
    <thead>
        <tr>
            <th>AIRPORT</th>
            <th>DEPARTING</th>
        </tr>
    </thead>
        <tbody>
    <% flights.forEach(function(f) { %>
    <tr>
        <td><%= f.airport %></td>
        <td><%= f.departs %></td>
        <td><a href="/flights/<%= f._id %>">DETAILS</a></td>
    </tr>
    <% }); %>
    </tbody>
</table>

<%- include('../partials/footer') %>